    %vpi_call/w 3 24 "$display", "Instr: 0x%08X, immSrc: %b -> ImmExt: 0x%08X (Expected: 0x%08X)", v0x560518af0470_0, v0x560518af0390_0, v0x560518af0560_0, v0x560518af02b0_0 {0 0 0};
    %load/vec4 v0x560518af0560_0;
    %load/vec4 v0x560518af02b0_0;
    %cmp/ne;
    %jmp/0xz  T_0.0, 6;
    %vpi_call/w 3 27 "$display", "\342\235\214 ERROR: Expected 0x%08X, but got 0x%08X", v0x560518af02b0_0, v0x560518af0560_0 {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %vpi_call/w 3 29 "$display", "\342\234\205 Passed" {0 0 0};
T_0.1 ;
    %end;
    .scope S_0x560518ad85c0;
T_1 ;
